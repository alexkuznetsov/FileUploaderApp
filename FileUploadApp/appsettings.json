{
  "Serilog": {
    "WriteTo": [
      {
        "Name": "Async",
        "Args": {
          "configure": [
            {
              "Name": "File",
              "Args": {
                "path": "logs/log.txt",
                "rollingInterval": "Day"//,
                //"rollOnFileSizeLimit": true,
                //"outputTemplate": "{Timestamp:yyyy-MM-dd HH:mm:ss.fff zzz} [{Level:u3}] {Message:lj}{NewLine}{Exception}",
                //"buffered": true
              }
            }
          ]
        }
      }
    ]
  },

  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },

  "AllowedHosts": "*",

  "fileStore": {
    "BasePath": "d:\\temp\\uploads"
  },

  "conf": {
    "AllowedContentTypes": [
      "image/jpeg",
      "image/png",
      "image/bmp",
      "image/x-windows-bmp",
      "image/gif",
      "image/tiff",
      "application/x-7z-compressed"
    ],

    "Mappings": {
      "iVBORw": "image/png",
      "/9j/4A": "image/jpeg",
      "Qk0=": "image/bmp",
      "SUkq": "image/tiff",
      "R0lG": "image/gif",
      "N3q8rw==": "application/x-7z-compressed"
    }
  }
}
